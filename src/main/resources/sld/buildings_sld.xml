<?xml version="1.0" encoding="utf-8"?>
<StyledLayerDescriptor version="1.0.20">
  <NamedLayer>
    <Name>buildings</Name>
    <UserStyle>
      <Name>buildings</Name>
      <FeatureTypeStyle>
        <Rule>
          <Filter>
          <And>
            <PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>yes</Literal>
            </PropertyIsEqualTo>
            <PropertyIsNotEqualTo>
              <PropertyName>aeroway</PropertyName>
              <Literal>terminal</Literal>
            </PropertyIsNotEqualTo>
            </And>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#bca9a9</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>damaged</Literal>
            </PropertyIsEqualTo>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#bca9a9</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#ff9933</CssParameter>
              <CssParameter name="stroke-width">0.5</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>collapsed</Literal>
            </PropertyIsEqualTo>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#bca9a9</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#ff0000</CssParameter>
              <CssParameter name="stroke-width">0.5</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <PropertyIsEqualTo>
              <PropertyName>aeroway</PropertyName>
              <Literal>terminal</Literal>
            </PropertyIsEqualTo>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill">#cc99ff</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke-width">0.2</CssParameter>
              <CssParameter name="stroke">#330066</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <PropertyIsEqualTo>
              <PropertyName>aeroway</PropertyName>
              <Literal>hangar</Literal>
            </PropertyIsEqualTo>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill">#bca9a9</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
  <NamedLayer>
    <Name>sportsFields</Name>
    <UserStyle>
      <Name>sportsFields</Name>
      <FeatureTypeStyle>
        <Rule>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#33cc99</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule> 
      </FeatureTypeStyle>
    </UserStyle>		
  </NamedLayer>	
  <NamedLayer>
    <Name>cemeteries</Name>
    <UserStyle>
      <Name>cemeteries</Name>
      <FeatureTypeStyle>
        <Rule>
          <PropertyIsEqualTo>
              <PropertyName>amenity</PropertyName>
              <Literal>grave_yard</Literal>
          </PropertyIsEqualTo>
          <PolygonSymbolizer>
            <Fill>
            	<GraphicFill>
					<Graphic>
						<ExternalGraphic>
							<href>/symbols/osm/grave_yard.png</href>
							<Format>png</Format>
						</ExternalGraphic>
						<Size>20</Size>
					</Graphic>
				</GraphicFill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#aacbaf</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule> 
        <Rule>
          <PropertyIsEqualTo>
              <PropertyName>landuse</PropertyName>
              <Literal>cemetery</Literal>
          </PropertyIsEqualTo>
          <PolygonSymbolizer>
            <Fill>
            <GraphicFill>
				<Graphic>
						<ExternalGraphic>
							<href>/symbols/osm/grave_yard_generic.png</href>
							<Format>png</Format>
						</ExternalGraphic>
						<Size>20</Size>
					</Graphic>
				</GraphicFill>
              <CssParameter name="fill-opacity">0.7</CssParameter>
              <CssParameter name="fill">#aacbaf</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule> 
      </FeatureTypeStyle>
    </UserStyle>		
  </NamedLayer>	  	
</StyledLayerDescriptor>